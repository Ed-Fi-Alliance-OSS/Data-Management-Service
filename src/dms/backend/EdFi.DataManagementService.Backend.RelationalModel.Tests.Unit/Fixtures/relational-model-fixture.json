{
  "apiSchemaVersion": "1.0.0",
  "projectSchema": {
    "projectName": "Ed-Fi",
    "projectEndpointName": "ed-fi",
    "projectVersion": "5.0.0",
    "resourceSchemas": {
    "schools": {
      "resourceName": "School",
      "isDescriptor": false,
      "allowIdentityUpdates": false,
      "arrayUniquenessConstraints": [
      ],
      "identityJsonPaths": [
        "$.schoolId",
        "$.schoolTypeDescriptor"
      ],
        "documentPathsMapping": {
          "SchoolId": {
            "isReference": false,
            "isPartOfIdentity": true,
            "isRequired": true,
            "path": "$.schoolId"
          },
          "SchoolTypeDescriptor": {
            "isReference": true,
            "isDescriptor": true,
            "isPartOfIdentity": true,
            "isRequired": true,
            "projectName": "Ed-Fi",
            "resourceName": "SchoolTypeDescriptor",
            "path": "$.schoolTypeDescriptor"
          }
        },
        "jsonSchemaForInsert": {
          "type": "object",
          "properties": {
            "schoolId": {
              "type": "integer"
            },
            "schoolTypeDescriptor": {
              "type": "string",
              "maxLength": 306
            },
            "name": {
              "type": "string",
              "maxLength": 60
            },
            "addresses": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "streetNumberName": {
                    "type": "string",
                    "maxLength": 150
                  },
                  "periods": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "beginDate": {
                          "type": "string",
                          "format": "date"
                        }
                      },
                      "required": [
                        "beginDate"
                      ]
                    }
                  }
                },
                "required": [
                  "streetNumberName"
                ]
              }
            },
            "_ext": {
              "type": "object",
              "properties": {
                "sample": {
                  "type": "object",
                  "properties": {
                    "customField": {
                      "type": "string",
                      "maxLength": 50
                    }
                  }
                }
              }
            }
          },
          "required": [
            "schoolId",
            "schoolTypeDescriptor",
            "name",
            "addresses"
          ]
        }
      }
    }
  }
}
