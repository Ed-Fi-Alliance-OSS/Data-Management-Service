{
    "AppSettings": {
        "DeployDatabaseOnStartup": false,
        "Datastore": "postgresql"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "IdentitySettings": {
        "AllowRegistration": true,
        "ConfigServiceRole": "service-role",
        "ClientRole": "dms-client",
        "Authority": "http://localhost/realms/dms",
        "Audience": "account",
        "ClientId": "DmsConfigurationService",
        "ClientSecret": "testsecret",
        "RequireHttpsMetadata": false,
        "RoleClaimType": "role"
    },
    "AllowedHosts": "*"
}
